
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            parallel {
                stage("test 1") {
                    step {
                        echo " test 1 " 
                    }
                }
                stage ("test 2" ) {
                    step {
                        echo "test 2 " 
                    }
                }
            }
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
